                    .project
                        #card_maker_tab.tab-background
                            .project-tab View Project
                        a#card_maker.project-link(href="https://deelinnell.github.io/pokemon-card-maker/", target="_blank")
                            #card_maker_trans.project-trans-box
                                p#card_maker_title.project-title Card Maker
                                .card_maker_background
                                    - const rainDrops = 75
                                    #cloud_rain_box
                                        - for (let r = 0; r < rainDrops; r++)
                                            svg.rain(style=`--x: ${Math.floor(Math.random() * 95)}; --t: ${Math.floor(Math.random() * 25)}; --a: ${Math.random() + 0.5}; --s: ${Math.random()};`)
                                                defs
                                                    linearGradient#rain_gradient(gradientTransform="rotate(90)")
                                                        stop(offset="30%", stop-color="hsla(189, 27%, 44%, 0.1)")
                                                        stop(offset="90%", stop-color="hsla(189, 27%, 44%, 0.3)")
                                                path.rain-drop(d="M15 3 Q16.5 6.8 20 25 A2.8 2.8 0 1 1 10 26 Q13.5 6.8 15 3z")
                                    #cloud_1
                                    #cloud_2
                                    p#card_maker_title_2.project-title Card Maker
                        a#card_maker_code.code-link(href="https://github.com/deelinnell/pokemon-card-maker", target="_blank")
