doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", type="text/css", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css")
        link(rel="stylesheet", type="text/css", href="style/style.css")
        script(src="script/index.js", defer)
        title My Portfolio
    body
        .container
            .header
            .container-main
                .fixed-border-container
                    .fixed-border
                .container-profile
                    .name-banner
                        .name-banner_front-end
                            .front-end_title FRONT END
                            .front-end_stroke FRONT END
                            h2 Developer
                        .name-banner_title(aria-hidden="true") Dallan Linnell
                        .name-banner_fixed
                            .name-banner_fixed-left
                                .name-banner_image
                            .name-banner_fixed-right
                        h1.name-banner_title.name-banner_stroked-title Dallan Linnell
                .container-projects
                    .project
                        #bitburner_tab.tab-background
                            .project-tab View Scripts
                        a#bitburner.project-link(href="https://github.com/deelinnell/my-bitburner-scripts", target="_blank")
                            #bitburner_hover
                            .project-trans-box
                                #rain_box
                                p#bitburner_title.project-title Bitburner Scripts
                        a#bitburner_code.code-link(href="https://github.com/deelinnell/my-bitburner-scripts", target="_blank")
                            - for (let i = 0; i < 35; i++)
                                .cell
                            .code-trans-box
                                - for (let i = 0; i < 6; i++)
                                    p.code-text CODE
                                p.code-title.code-text CODE
                        .code-tab-background
                            .code-tab View Code
                    .project
                        #fullpage_tab.tab-background
                            .project-tab View Project
                        a#fullpage.project-link(href="https://deelinnell.github.io/fullpage-fantasy-website/", target="_blank")
                            - for (let i = 0; i < 11; i++)
                                .width-cell
                            #fullpage_trans_box.project-trans-box
                                #dragon_head
                                #dragon_gradient
                                #fire_box
                                    svg#fire_container(height="100px", width="580px")
                                        defs
                                            radialGradient#fire_gradient(fx="23%")
                                                stop(offset="10%", stop-color="rgb(255, 236, 207)")
                                                stop(offset="50%", stop-color="rgb(255, 230, 0)")
                                        path#fire_triangle
                                p#fullpage_title_bottom.project-title Fullpage.js Site
                                p#fullpage_title.project-title Fullpage.js Site
                        a#fullpage_code.code-link(href="https://github.com/deelinnell/fullpage-fantasy-website", target="_blank")
                            - for (let i = 0; i < 35; i++)
                                .cell
                            .code-trans-box
                                - for (let i = 0; i < 6; i++)
                                    p.code-text CODE
                                p.code-title.code-text CODE
                        .code-tab-background
                            .code-tab View Code
                    .project
                        #mobile_phone_tab.tab-background
                            .project-tab View Project
                        a#mobile_phone.project-link(href="https://responsive-business-website.vercel.app/", target="_blank")
                            #mobile_trans.project-trans-box
                                #mobile_background
                                p#mobile_phone_title_1.project-title Mobile Phone Site
                                p#mobile_phone_title_2.project-title Mobile Phone Site
                                p#mobile_phone_title_3.project-title Mobile Phone Site
                                p#mobile_phone_title_4.project-title Mobile Phone Site
                                p#mobile_phone_title.project-title Mobile Phone Site
                                #cracked_glass
                        a#mobile_phone_code.code-link(href="https://github.com/deelinnell/responsive-business-website", target="_blank")
